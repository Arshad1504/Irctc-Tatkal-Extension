{
    "manifest_version": 3,
    "name": "IRCTC Tatkal Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.irctc.co.in/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["storage", "alarms"]
  }
  